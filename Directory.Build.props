<Project>

    <PropertyGroup>
        <NoWarn>$(NoWarn);1591</NoWarn>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference
                Include="StyleCop.Analyzers"
                Version="1.2.0-beta.435"
                PrivateAssets="all"
                Condition="$(MSBuildProjectExtension) == '.csproj'"/>
        <PackageReference
                Include="SonarAnalyzer.CSharp"
                Version="9.0.0.68202"
                PrivateAssets="all"
                Condition="$(MSBuildProjectExtension) == '.csproj'"/>
    </ItemGroup>

</Project>
